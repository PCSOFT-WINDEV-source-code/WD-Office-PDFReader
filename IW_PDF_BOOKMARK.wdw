   -
     name : LoadPDF
     internal_properties : CAAAAAgAAAACQ53Vux4CrTY2d/A/dXt2+MWqpfonuZC1jxrEGFWAz/437fGHw2lY+SrzYWgcTsmkHzFQqnzvUo39gnwZUO/i573V+qpoHTzFhoRnXXCVGgG+3eQn6r0ePF4rfJ3INfyhnoy/5OEuRbpOJr3Z
     procedure_id : 1404998877649103640
     type_code : 14
     code : |1+
      // Summary: Loads PDF bookmarks
      // Syntax:
      //LoadPDF (<PDFReader> is Control)
      //
      // Parameters:
      //	PDFReader (Control): Associated PDF Reader control
      // Return value:
      // 	None
      //
      procedure LoadPDF( PDFReader is Control )
      
      // Stores the associated PDF Reader control and loads the document
      Reader <- PDFReader
      docPDF is pdfDocument = Reader..Value
      
      // Initializes the display
      AllBookmarks.DeleteAll()
      TVT_BOOKMARK.DeleteAll()
      
      // If the document has no bookmarks
      if docPDF.Bookmark..Count = 0 then
      	return
      end
      
      TVT_BOOKMARK.LoadingInProgress = True
      
      // Stores the maximum height for a row that contains a bookmark
      nMax is int
      nMax = MaxRowHeight()
      
      // Lists the bookmarks
      __ListBookmarks( docPDF.Bookmark )	
      
      // Deletes the initial selection
      TVT_BOOKMARK.SelectMinus()
      TVT_BOOKMARK.LoadingInProgress = False
      
      	INTERNAL PROCEDURE __ListBookmarks( arrBookmarksEx, nParent is int = 0 )
      		
      		stBookmark	is pdfBookmark
      		nRow		is int
      		
      		// If there are no child bookmarks, removes the "expanded" and "collapsed" images
      		if arrBookmarksEx..Count = 0 then
      			if nParent > 0 then
      				TVT_BOOKMARK[ nParent ].ImageDéroulée	= Null
      				TVT_BOOKMARK[ nParent ].ImageEnroulée	= Null
      			end
      			return
      		end
      		
      		// For each bookmark
      		for each stBookmark of arrBookmarksEx
      			
      			AllBookmarks.Add(stBookmark)
      			
      			// Parent bookmarks
      			if nParent = 0 then
      				
      				nRow							= TVT_BOOKMARK.AddLine(stBookmark.Title)
      				// Parents are collapsed by default
      				TVT_BOOKMARK[ nRow ].Enroulé	= False
      				
      			else
      				
      				nRow = TVT_BOOKMARK.AddChild(nParent, stBookmark.Title)
      				
      			end
      			
      			// By default, adds the "collapsed" and "expanded" images
      			TVT_BOOKMARK[ nRow ].ImageDéroulée	= IMG_Expanded
      			TVT_BOOKMARK[ nRow ].ImageEnroulée	= IMG_Collapsed
      			
      			// Adapts the row height to the title of the bookmark
      			TVT_BOOKMARK[ nRow ].Hauteur		= Min(nMax, Max(CoordinateEditorToScreen(32), TextHeight(STC_DEFAULT, stBookmark.Title, tWithMargin + tMultiline) + CoordinateEditorToScreen(8)))
      			
      			// Enumerates the children
      			__ListBookmarks( stBookmark.ChildBookmark, nRow )
      			
      		end
      		
      	end
     type : 458752
   -
     name : ResizeRow
     internal_properties : CAAAAAgAAAAw3ciPMuWjU4J5ex0LTlIGE7vkL7I+ujMoW20FOISyBd4lzZ5MnyKqIGjHJoiF6B45vlYKqkLYhL86DFiCbUITtT35hSGgTQzGYeZpouxH+LtX1TgBoSEw
     procedure_id : 1404998877649169176
     type_code : 14
     code : |1+
      // Summary: Resizes rows automatically
      // Syntax:
      //ResizeRow ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //// Automatic procedure:
      // The procedure is executed manually, during a call in the code, with a timeout of 20 hundredths of a second
      // 
      
      procedure ResizeRow()
      
      nMax is int
      nMax						= MaxRowHeight()
      
      TVT_BOOKMARK.DisplayEnabled	= False
      
      for each ligne nRow of TVT_BOOKMARK
      	// Adapts the row height to the title of the bookmark
      	TVT_BOOKMARK[ nRow ].Hauteur = Min(nMax, Max(CoordinateEditorToScreen(32), TextHeight(STC_DEFAULT, TVT_BOOKMARK[nRow].COL_CAPTION, tWithMargin + tMultiline) + CoordinateEditorToScreen(8)))
      end
      
      TVT_BOOKMARK.DisplayEnabled = True
     type : 458752
   -
     name : MaxRowHeight
     procedure_id : 1404998877649234712
     type_code : 14
     code : |1-
      // Summary: Returns the maximum height available for a row that contains a bookmark
      // Syntax:
      //[ <Result> = ] MaxRowHeight ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	8-byte int: Maximum height allowed
      //
      procedure MaxRowHeight()
      
      // Allows a maximum of 4 rows
      return TextHeight(STC_DEFAULT, "Row 1[%RC%]Row 2[%RC%]Row 3[%RC%]Row 4", tWithMargin + tMultiline) + CoordinateEditorToScreen(8)
     type : 458752
