     code : |1-
      // Summary: Opens a document
      // Syntax:
      //MenuOpenDocument (<sPath> is string [, <bReuseWindow> is boolean [, <xDocPos> is numeric]])
      //
      // Parameters:
      //	sPath (ANSI string): Full document path
      //	bReuseWindow (boolean - default value=0): True to recycle the current window, False otherwise
      //	xDocPos (numeric - default value=0): Position saved in the document
      // Return value:
      // 	None
      //
      // Automatic process of exceptions:   run CASE EXCEPTION:
      //
      
      Procédure MenuOpenDocument(local sPath is string, bReuseWindow is boolean = False, local xDocPos is numeric = 0)
      
      sAlias is string
      bReadOnly is boolean = ( BTN_MENU_NEW..Visible = False _and_ BTN_MENU_SAVE..Visible = False )
      bFromExplorerDrop is boolean = ( _EVE.wMessage = 0x0233 ) // WM_DROPFILES
      bOpenNewWindow is boolean = False
      
      HourGlass( True )
      
      // If attempt to open the document currently opened
      if sPath ~= gsDocumentPath then
      	
      	// Closes the file menu
      	FileMenuClose( (not bOpenNewWindow) )
      	
      	// Abandon
      	return
      end
      
      // If no document is opened and if the creation is not enabled / or if a file was dropped at start / if a document was created without modifying it
      if bReuseWindow _or_ ( bFromExplorerDrop _and_ gsDocumentPath = "" ) _or_ ( gsDocumentPath = "" _and_ bReadOnly ) _or_ ( gsDocumentPath = "" _and_ gbModifiedDocument = False ) then
      	
      	// Open instead
      	sAlias = gDocumentWin..Alias = "" ? gDocumentWin..Name else gDocumentWin..Alias
      	
      	// If a file was dropped from the explorer or if the window is recycled
      	if bReuseWindow _or_ bFromExplorerDrop then
      		
      		// If the current document was not saved
      		if MenuCloseDocument( False ) = False then
      			
      			// Abandon
      			return
      		end
      	end		
      else
      	
      	sAlias = "DOC_" + SysDateTime		
      	
      	// Opens a window alias
      	OpenSister(sAlias + " = " + gDocumentWin..Name + NoFocus)
      	
      	// Maximizes the window if we are maximized
      	if WinSize(gDocumentWin) = MaxSize then WinSize(sAlias, MaxSize)
      	
      	// Store the fact that another window is opened
      	bOpenNewWindow = True
      end
      
      // Takes a reference on the alias actions
      pstDocumentActions is ACTION dynamic = {sAlias + ".DocumentAction", indVariable}
      
      // Opens the file
      if pstDocumentActions.OpenDocument = Null _or_ pstDocumentActions.OpenDocument( sPath, xDocPos ) = True then
      	
      	// Adds the file to the list of recent documents
      	SysRecentDocAdd( sPath )
      	
      	// Stores the document in the MRU
      	MRUAdd( sPath )
      	
      	// Loads the file information
      	ExecuteProcess(sAlias + ".MenuDisplayInfo", trtProcedure, ( sPath ))
      	
      	// Gives focus to the window
      	SysSetFocus( Handle(sAlias) )
      end
      
      // Closes the file menu
      FileMenuClose( (not bOpenNewWindow) )
      
      case exception:
      dbgAssert( False, ExceptionInfo )
      return
      
      end:
      
      HourGlass( False )
      
      if bFromExplorerDrop then
      	_EVE.Return = False
      end
     type : 458752
