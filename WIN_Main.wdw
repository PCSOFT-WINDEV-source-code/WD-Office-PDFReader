   -
     code : |1+
      Procédure MyWindow()
      
      gsPassword is string // Password of the file
   -
     code : |1+
      // By default, no document is opened in the reader: the options for handling documents must be grayed in the menu
      BTN_MENU_PRINT..State = Grayed
      BTN_MENU_EXPORT..State = Grayed
      BTN_SHORT_PRINT..State = Grayed
      // Hides the protection information as well
      GR_Protection..Visible = False
      
      
      // Code specific to opening
      DocumentAction.OpenDocument = __OpenPDF
      
      	INTERNAL PROCEDURE __OpenPDF( sPath is string, xDocPos is numeric = 0 ) : boolean
      	
      	// Asks for the PDF password if necessary
      	if PDFAskForPassword(sPath) = False then result False
      	
      	// Loads the file in the PDF reader
      	if PDFReaderOpen(PDF_PDFReader, sPath, gsPassword) = False then result False
      	
      	// Now that a document is opened, enable the options
      	BTN_MENU_PRINT..State = Active
      	BTN_MENU_EXPORT..State = Active
      	BTN_SHORT_PRINT..State = Active
      	// Hides the protection information as well
      	GR_Protection..Visible = True
      	
      	// If the display parameters must be restored
      	IF SET_SETTING_PDF.gbRestoreDisplayParameters THEN
      		// Retrieves the additional information specific to PDF
      		nDisplayMode, nZoom are int
      		(nDisplayMode, nZoom) = AdditionalInformationGet( sPath )
      		// Applies the information (display mode, zoom)
      		IF nDisplayMode <> -1 THEN PDF_PDFReader..DisplayMode = nDisplayMode
      		IF nZoom <> -1 THEN PDF_PDFReader..Zoom = nZoom
      	END
      	
      	// If no position is requested
      	IF xDocPos <= 0 THEN
      		// Checks in MRU whether this document was already opened and retrieves its position
      		xDocPos = MenuGetLastPosition(sPath)
      	END
      	
      	// Displays the stored page
      	IF xDocPos > 0 THEN
      		// As long as this page is not accessible
      		WHILE xDocPos > PDF_PDFReader..NumberAccessiblePages
      			// Waits for the PDF to be loaded
      			Wait(100 ms, waitRedraw)
      		END
      		// Displays the requested page
      		PDF_PDFReader..NumberDisplayedPage = xDocPos
      	END
      	
      	// If the document has bookmarks, they should be displayed
      	BookmarkAvailable()
      	
      	// The file was successfully opened
      	result True
      	
      	end
      
      
      // By default, hides the bookmarks area
      BookmarkDisplay(False)
      
      // If a document is passed in parameter
      IF CommandLine() <> "" THEN
      	// Opens the requested document
      	MenuOpenDocument(Replace(CommandLine(), """",""))
      END
      
      END:
      
      // Displays the default reader plane
      MyWindow.Plane = PLAN_PDFREADER
      
      // Initializes the ribbon
      oRibbon				is Control		<- PDF_PDFReader.Ribbon
      ProcedureInitRibbon	is procédure	= pHideCallbackRibbon
      InitializeRibbon(oRibbon..FullName, <pHideCallbackRibbon>:ProcedureInitRibbon)
      
      	INTERNAL PROCÉDURE pHideCallbackRibbon(oNativeRibbon is Control)
      		
      		// Displays the first group
      		sGroupName is string = EnumControl(oNativeRibbon[1]..FullName, 1)
      		IF sGroupName <> "" THEN
      			{oNativeRibbon..FullName+"."+sGroupName, indControl}..Visible = True
      		END
      		
      		// Hides the General group
      		sControlName is string = oNativeRibbon..FullName + ".RIBGRP_WDAAF_General"
      		IF ControlExist(sControlName) THEN
      			oGroupOpen is Champ <- {sControlName, indControl}
      			IF oGroupOpen <> Null THEN
      				oGroupOpen..Visible = False
      			END
      		END
      		
      	END
      
