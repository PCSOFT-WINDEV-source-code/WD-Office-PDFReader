        code : |1-
         
         nRow is int = MySelf.InfoXY(liLineNumber, MouseXPos, MouseYPos)
         if nRow > 0 then
         	
         	// This line is already hovered
         	if Val( MySelf..Note ) = nRow then
         		return
         	end
         	
         	if Val( MySelf..Note ) > 0 then
         		MySelf[ Val( MySelf..Note ) ]..BackgroundColor = Transparent
         	end	
         	
         	oColor is a Color = STC_BCKGRD_TITLE_BAR..BackgroundColor
         	oColor..Saturation				= 35
         	oColor..Luminance				= 220
         	
         	// Highlights the hovered line
         	MySelf[ nRow ]..BackgroundColor	= oColor
         	MySelf..Note					= nRow
         	
         	// Rollover tooltip
         	MySelf..ToolTip					= ""
         	MySelf..ToolTip					= ATT_FILE[ nRow ]
         	
         else
         	
         	if Val( MySelf..Note ) > 0 then
         		MySelf[ Val( MySelf..Note ) ]..BackgroundColor = Transparent
         	end	
         	
         	MySelf..Note	= ""
         	
         	// Rollover tooltip
         	MySelf..ToolTip	= ""
         end
