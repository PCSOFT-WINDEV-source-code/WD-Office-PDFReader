      Procédure MenuDisplayInfo(local sPath is string)
      
      //Runs the process defined in the template
      ExecuteAncestor(sPath)
      
      // If the p ath is not valid
      if sPath ~= "" then
      	
      	// Default caption
      	STC_INFO_FILEPROTECTION = "No file is opened"
      	// The PDF is read-only: if no document is opened, the template captions must be modified
      	STC_INFO_CREATIONDATE = "No file is opened"
      	STC_INFO_MODIFICATIONDATE = "No file is opened"
      	STC_INFO_SIZE = "No file is opened"
      	
      else
      	
      	// If the file is protected (in reading)
      	if PDFIsProtected(sPath) then
      		STC_INFO_FILEPROTECTION = "The file is password protected (reading)"
      	else // No password
      		STC_INFO_FILEPROTECTION = "The file is not password protected (reading)"
      	end
      	
      end
      
      // If the file is protected
      let bProtectedFile = (fFileExist(gsDocumentPath) _and_ PDFIsProtected(gsDocumentPath))
      
      // Update the window title (adds protected PDF)
      MyWindow..Title = ( gsNameOpenedDocument + (gbModifiedDocument ? "*" else "") + (bProtectedFile ? " [PROTECTED]" else "") )  + [" - "] + NoSpace(ExtractString(ProjectInfo(piProjectName), 2, "WD Office "))
