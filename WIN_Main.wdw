     name : BookmarkAvailable
     procedure_id : 1407937872293220901
     type_code : 14
     code : |1+
      // Summary: Checks for bookmarks in the document and adapts how the content is displayed
      // Syntax:
      // BookmarkAvailable ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure BookmarkAvailable()
      
      // Retrieves the displayed document
      docPDF					is pdfDocument	= PDFopen(PDF_PDFReader, gsPassword)
      
      // Shows or hides the bookmark area and button
      bDocumentWithBookmark	is boolean		= ( docPDF.Bookmark..Count > 0 )
      BookmarkDisplay(bDocumentWithBookmark)
      BTN_MENU_BOOKMARKS.Visible = bDocumentWithBookmark
     type : 458752
   -
     name : BookmarkDisplay
     procedure_id : 1407937876588254498
     type_code : 14
     code : |1+
      // Summary: Shows or hides the bookmark area
      // Syntax:
      //BookmarkDisplay ( [<bDisplay> is boolean])
      //
      // Parameters:
      //	bDisplay (boolean): True to show the bookmark area, False to hide it
      // Return value:
      // 	None
      //
      procedure BookmarkDisplay(bDisplay is boolean = BTN_MENU_BOOKMARKS.Pushed)
      
      // Applies the state to the button
      BTN_MENU_BOOKMARKS.Pushed = bDisplay
      
      // If bookmarks are to be displayed, updates the control
      if bDisplay then
      	IWC_Bookmarks.LoadPDF( PDF_PDFReader )	
      end
      
      // Shows or hides the bookmark area
      if bDisplay then
      	
      	IWC_Bookmarks.Visible	= True
      	SPLIT_NoName1.Visible	= True
      	SPLIT_NoName1.X			= 250
      	
      else
      	
      	SPLIT_NoName1.X			= 0
      	IWC_Bookmarks.Visible	= False
      	SPLIT_NoName1.Visible	= False
      end
