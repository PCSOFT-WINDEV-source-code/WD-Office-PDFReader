      // Summary: Opens the file picker
      // Syntax:
      //[ <Result> = ] OpenFilePicker ([<nMode> is int [, <sInitialDirectory> [, <sFilter> is string [, <sDefaultExtension> is string]]]])
      //
      // Parameters:
      //	nMode (integer - default value=2): Picker mode (fSelXXX constants)
      //	sInitialDirectory (default value=""): Opening directory (by default, last opened directory or directory of current document)
      //	sFilter (ANSI string - default value=""): Filter of file picker
      // 	sDefaultExtension (ANSI string - default value=""): Default extension (without *.)
      // Return value:
      // 	ANSI string: Selected path
      //
      Procédure OpenFilePicker(nMode is int = fselOpen, local sInitialDirectory = "", local sFilter is string = "", local sDefaultExtension is string = "") : string
      
      sFile is string
      sInitialDirectory = (sInitialDirectory <> "" ? sInitialDirectory else LoadParameter("Recent\LastPath", ( gsDocumentPath = "" ? SysDir( srMyDocuments ) else fExtractPath(gsDocumentPath, fDrive + fDirectory) )))
      
      // If no default filter was specified
      if sFilter = "" then
      	sFilter = "[%SET_SETTING.DESCRIPTION_DOCUMENT%] ([%SET_SETTING.EXTENSION_DOCUMENT_PICKER%])" + TAB + SET_SETTING.EXTENSION_DOCUMENT_PICKER
      end
      
      // If no default extension was specified
      if sDefaultExtension = "" then
      	sDefaultExtension = ExtractString(Replace( SET_SETTING.EXTENSION_DOCUMENT_PICKER , "*.", "" ), 1, ";")
      end
      
      // Opens the file picker
      sFile = fSelect(sInitialDirectory, gsNameOpenedDocument, (nMode & fselCreate = fselCreate ? "Save as" else "Open"), "All the files (*.*)" + TAB + "*.*" + CR + ...
      sFilter, sDefaultExtension, nMode )
      
      // If no file was selected
      if sFile = "" then result ""
      
      // Stores the path
      SaveParameter("Recent\LastPath", fExtractPath(sFile, fDrive + fDirectory))
      
      result sFile
