     procedure_id : 1898161032729623317
     type_code : 14
     code : |1-
      // Summary: Lists the documents recently opened
      // Syntax:
      //MenuListRecentDocuments ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procédure MenuListRecentDocuments()
      
      HourGlass( True )
      
      // Disables the display
      LOOP_RECENT_DOCUMENTS..DisplayEnabled = False
      BTN_OPEN_BROWSE..Visible = False
      
      // Signals the start of fill
      LOOP_RECENT_DOCUMENTS.StartFilling()
      
      // Clears the list
      LOOP_RECENT_DOCUMENTS.DeleteAll()
      
      // The MRU is already sorted
      FOR EACH stElement of SET_SETTING.MRU
      	
      	// If the file extension does not correspond to the one that was configured, ignore it
      	if Position(SET_SETTING.EXTENSION_DOCUMENT_PICKER, "*." + fExtractPath(stElement.FullPath, fExtension)[[2 to]], 1, IgnoreCase + WholeWord) < 1 then continue
      	
      	// If the document does not exist anymore, ignore it
      	if fFileExist(stElement.FullPath) = False then continue
      	
      	dtDate is datetime = DateTimeUTCToLocal( stElement.AccessDateTime )
      	
      	// Loads the file information of document				
      	nRow is int = LOOP_RECENT_DOCUMENTS.addline( stElement.FullPath, DateToString(dtDate..Date, maskDateRelativeDuration), *, stElement.DocPosition)
      	
      	LOOP_RECENT_DOCUMENTS[nRow].STC_DATE = dtDate
      	LOOP_RECENT_DOCUMENTS[nRow].STC_FOLDER = UncompleteDir(fExtractPath(stElement.FullPath, fDrive + fDirectory))
      	LOOP_RECENT_DOCUMENTS[nRow].STC_FILE_NAME = fExtractPath(stElement.FullPath, fFileName + fExtension)
      end
      
      // Signals the end of fill
      LOOP_RECENT_DOCUMENTS.EndFilling()
      LOOP_RECENT_DOCUMENTS.SelectMinus()
      
      // Re-enable the display
      BTN_OPEN_BROWSE..Visible = True
      LOOP_RECENT_DOCUMENTS..DisplayEnabled = True
      
      HourGlass( False )
     type : 458752
